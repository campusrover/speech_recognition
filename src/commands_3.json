{
    "go": {
        "forward": { 
            "receiver": "/cmd_vel",
            "type": "geometry_msgs/Twist",
            "msg" : {
                "linear": {
                    "x": 0.2,
                    "y": 0.0,
                    "z": 0.0
                },
                "angular": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                }
            }
        },
        "back": { 
            "receiver": "/cmd_vel",
            "type": "geometry_msgs/Twist",
            "msg" : {
                "linear": {
                    "x": -0.2,
                    "y": 0.0,
                    "z": 0.0
                },
                "angular": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                }
            }
        }
    },
    "turn": {
        "left": { 
            "receiver": "/cmd_vel",
            "type": "geometry_msgs/Twist",
            "msg" : {
                "linear": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                },
                "angular": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.1
                }
            }
        }, 
        "right": { 
            "receiver": "/cmd_vel",
            "type": "geometry_msgs/Twist",
            "msg" : {
                "linear": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                },
                "angular": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.1
                }
            }
        }
    }, 
    "stop": {
        "moving": { 
            "receiver": "/cmd_vel",
            "type": "geometry_msgs/Twist",
            "msg" : {
                "linear": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                },
                "angular": {
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                }
            }
        }
    }, 
    "gripper": {
        "open": {
            "receiver": "/gripper",
            "type": "std_msgs/Bool",
            "msg" : {
                "data": true
            }
        },
        "close": {
            "receiver": "/gripper",
            "type": "std_msgs/Bool",
            "msg" : {
                "data": false
            }
        }
    }
}