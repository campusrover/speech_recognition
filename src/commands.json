{
    "drive": 
    {
        "forward": 
        {
            "linear.x": 0.1,
            "angular.z": 0
        },
        "straight":
        {
            "linear.x": 0.1,
            "angular.z": 0
        }, 
        "back":
        {
            "linear.x": -0.1,
            "angular.z": 0
        }, 
        "topic": "/cmd_vel",
        "message_type": "geometry_msgs/Twist"

    },  
    "go": 
    {
        "forward": 
        {
            "linear.x": 0.1,
            "angular.z": 0
    },
        "straight":
        {
            "linear.x": 0.1,
            "angular.z": 0
    }, 
        "back":
        {
            "linear.x": -0.1,
            "angular.z": 0
        }, 
        "topic": "/cmd_vel", 
        "message_type": "geometry_msgs/Twist"
    }, 
    "turn":
    {
        "left":
        {
            "linear.x": 0,
            "angular.z": -0.1
        },
        "right":
        {
            "linear.x": 0,
            "angular.z": 0.1
        }, 
        "topic": "/cmd_vel", 
        "message_type": "geometry_msgs/Twist"
    }, 
    "open gripper": {
        "data": true,
        "topic": "/gripper",
        "message_type": "std_msgs/Boolean"
    },
    "close gripper": {
        "data": false,
        "topic": "/gripper",
        "message_type": "std_msgs/Boolean"
    },
    "stop": {
        "linear.x": 0,
        "angular.z": 0, 
        "topic": "/cmd_vel",
        "message_type": "geometry_msgs/Twist"
    },
    
